<%= render 'post', post: @post %>
    <% if @post.user == current_user %>
      <div class="text-center edit-links">
        <%= link_to "Back", posts_path %>
        |
        <%= link_to "Edit Post", edit_post_path(@post) %>
        |
        <%= link_to "Upload Models", new_post_upload_path(@post, @upload) %>
      </div>
    <% else %>
      <div class="text-center edit-links">
        <%= link_to "Back", posts_path %>
      </div>
    <% end %>
  </div>
</div>